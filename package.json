{
  "name": "lovue-playgrounds-service",
  "description": "NodeJS server with JWT, MongoDB, Social-login, File-uploader and Send-mail",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "keywords": [
    "lovue",
    "server",
    "express",
    "mongodb",
    "jwt",
    "jsonwebtoken",
    "passport",
    "multer",
    "babel",
    "es6",
    "es2015",
    "es2016",
    "es2017",
    "jest",
    "eslint"
  ],
  "author": "mornya <mornya78@gmail.com>",
  "homepage": "https://lovue-playgrounds-service.herokuapp.com",
  "repository": "https://bitbucket.org/lovue/lovue-playgrounds-service",
  "bugs": "",
  "main": "dist/index.js",
  "scripts": {
    "prestart": "npm run clean && mkdir -p dist && npm run -s build",
    "start": "pm2 start dist/index.js --name lovue-playgrounds-service",
    "stop": "pm2 delete lovue-playgrounds-service",
    "restart": "pm2 restart lovue-playgrounds-service",
    "dev": "nodemon src/index.js --exec \"node -r @babel/register --max_old_space_size=1024\"",
    "clean": "rimraf dist",
    "build": "babel src -s -D -d dist",
    "lint": "eslint --cache --cache-location node_modules/.cache/eslintcache --ignore-pattern .gitignore --ext js --max-warnings 100 ./src",
    "test": "jest --color true --watchAll",
    "heroku-prebuild": "cp .env.production .env",
    "heroku-postbuild": "mkdir -p dist && npm run -s build"
  },
  "dependencies": {
    "acorn": "^7.2.0",
    "body-parser": "^1.19.0",
    "chalk": "^4.0.0",
    "cookie-parser": "^1.4.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "fs-extra": "^9.0.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.17",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "nodemailer": "^6.4.8",
    "passport": "^0.4.1",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth": "^2.0.0",
    "passport-kakao": "^1.0.0",
    "passport-naver": "^1.0.6",
    "pug": "^3.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.2",
    "@babel/register": "^7.10.1",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.0.1",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-preset-minify": "^0.5.1",
    "eslint": "6.8.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.13.2",
    "eslint-watch": "^7.0.0",
    "jest": "^26.0.1",
    "nodemon": "^2.0.4",
    "rimraf": "^3.0.2",
    "supertest": "^5.0.0-0"
  },
  "engines": {
    "node": ">=10",
    "npm": ">=6"
  },
  "jest": {
    "testEnvironment": "node"
  }
}
